<%- include('header', {title:`note`}); -%>
  <body>
    <%- include('nav'); -%>
    <div class='container col-10 bg-light border mt-4'>
      <%- include('note-header', {behavior, date, time, flock_size}); -%>
      <p class= "text-justify text-primary">Laborum consectetur voluptate Lorem deserunt. Mollit minim sunt est in pariatur elit. Irure qui dolor nostrud ex et anim. Et eiusmod veniam velit irure est commodo. Dolore nulla aute eu consectetur eiusmod labore excepteur Lorem minim commodo. Enim velit ea cillum minim nostrud.</p>
      <% if(locals.reported){ %>
        <h6 class="text-primary">Reported: <%= reported%></h6>
        <% } %>  
        


    <div class="btn-group">
    <a href="/note/<%=id%>/edit" class="btn btn-outline-secondary btn-sm">Edit</a>
     <form
          action="/note/<%=id%>/delete?_method=DELETE"
          method="POST"
          class="d-inline-block "
        >
          <input
            type="submit"
            value="Delete"
            id="delete"
            class="btn btn-outline-secondary btn-sm"
           
          />
        </form>
    <a href="/note/<%=id %>/prev" class="btn btn-outline-secondary btn-sm">Prev</a>
    <a href="/note/<%=id %>/next" class="btn btn-outline-secondary btn-sm">Next</a>
    <a href="/note/<%=id%>?fav=<%=!fav %>" class="btn btn-outline-secondary btn-sm"
      <% if(fav){%>
        style ="color: coral;"
      <% } %>
      ><i class="fa fa-star" aria-hidden="true"></i></a>
    </div>
    </div>
    <script
      src="https://maps.googleapis.com/maps/api/js?key=AIzaSyA55CGjZZP9Yz82YjeR32VCCYU1zeQQPhA&callback=initMap&libraries=&v=weekly"
      async
    ></script>
  </body>
</html>
